{
    "name": "baytek/laravel-content-document",
    "description": "Document Content Provider",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Yvon Viger",
            "email": "yvon@baytek.ca"
        },
        {
            "name": "Sarah Marinoff",
            "email": "sarah@baytek.ca"
        }
    ],
    "autoload": {
        "psr-4": {
            "Baytek\\Laravel\\Content\\Types\\Document\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "baytek/laravel-content": "1.2.*"
    },
    "post-install-cmd": [
        "php artisan install:document"
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Baytek\\Laravel\\Content\\Types\\Document\\DocumentContentServiceProvider"
            ]
        }
    }
}
